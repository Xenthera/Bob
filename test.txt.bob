extension array {
  func sum() {
    var tally = 0;
    for (var i = 0; i < this.len(); i++) {
        tally += this[i];
    }
    return tally;
  }
}


func range(a, b){
    var ret_arr = [];
    for (var i = a; i < b; i++) {
        ret_arr.push(i);
    }
    return ret_arr;
}

func range(a){
    var ret_arr = [];
    for (var i = 0; i < a; i++) {
        ret_arr.push(i);
    }
    return ret_arr;
}



print(range(50,100));

print(range(10));

extension string {
    func echo(){
        print(this);
    }

    func echo(x){
        print(this + " " + x);
    }
}

"hello".echo();
"hello".echo("world");







