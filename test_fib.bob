var counter = 0;

func fib(n) {
    return fibTail(n, 0, 1);
}

func fibTail(n, a, b) {
    if (n <= 0) {
        return a;
    }
    counter++;  // Only increment for recursive calls
    return fibTail(n - 1, b, a + b);
}

print("Fibonacci test:");
var fib_result = fib(50);

print("Result: " + fib_result);

print("Counter: " + counter);


