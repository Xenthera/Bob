// Focused big number comparison test
print("=== Bob Big Number Comparison ===");

// Test 1: Large addition (should be fast)
var a = 1234567890123456789012345678901234567890;
var b = 9876543210987654321098765432109876543210;
var result = a + b;
print("Large addition result: " + result);

// Test 2: Large multiplication (should be slower)
var c = 123456789012345678901234567890;
var d = 987654321098765432109876543210;
var product = c * d;
print("Large multiplication result: " + product);

// Test 3: Iterative sum with big numbers
var sum = 0;
for (var i = 0; i < 100; i++) {
    sum = sum + 123456789012345678901234567890;
}
print("Iterative sum result: " + sum);

print("=== Test Complete ===");
