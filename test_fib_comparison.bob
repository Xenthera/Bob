// Test fib performance comparison with hybrid approach
func fib_iterative(n) {
    if (n <= 1) return n;
    var a = 0;
    var b = 1;
    for (var i = 2; i <= n; i++) {
        var temp = a + b;
        a = b;
        b = temp;
    }
    return b;
}

func fib_recursive(n) {
    if (n <= 1) return n;
    return fib_recursive(n - 1) + fib_recursive(n - 2);
}

print("Testing Bob fib performance with hybrid approach...");

// Test iterative (should be fast)
print("fib_iterative(40) = " + fib_iterative(40));

// Test recursive (will be slow due to O(2^n) algorithm)
print("fib_recursive(35) = " + fib_recursive(35));

print("Test completed!");
print("Note: Iterative is O(n), Recursive is O(2^n) - algorithm matters more than TCO!");
